<br>
<% if @friend.ankle_monitors.present? %>
  <% @friend.ankle_monitors.order('date_removed desc').each do |ankle_monitor| %>
    <%= render partial: 'admin/friends/ankle_monitors/ankle_monitor', locals: { friend: @friend, ankle_monitor: ankle_monitor } %>
  <% end %>
<% end %>

<script>
  $(document).ready(function () {
    $('.open_ankle_monitor_modal').click(function() {
      $('#ankle_monitor_modal').modal('show');
    });
  });
</script>
